{
    "source": {
        "include": [
            "src/"
         ],
        "exclude": [],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "docs/",
        "recurse": true,
        "package": "package.json",
        "readme": "README.md",
        "template": {
            "cleverLinks": false,
            "monospaceLinks": false,
            "default": {
                "outputSourceFiles": true
            },
            "systemName": "Elementjs",
            "footer": "",
            "copyright": "DocStrap Copyright Â© 2012-2013 The contributors to the JSDoc3 and DocStrap projects.",
            "navType": "vertical",
            "theme": "flatly",
            "linenums": true,
            "collapseSymbols": false,
            "inverseNav": true
        }
    }
}